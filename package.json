{
  "name": "tailgrids-vue",
  "description": "Tailwind CSS Vue Components based on TailGrids Core, comes with all essential UI components and elements you need to kickstart Tailwind + Vue web projects.",
  "version": "2.2.1",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "author": "TailGrids Team <hello@tailgrids.com> (https://tailgrids.com/)",
  "homepage": "https://tailgrids.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/TailGrids/tailgrids-vue"
  },
  "bugs": {
    "url": "https://github.com/TailGrids/tailgrids-vue/issues"
  },
  "license": "MIT",
  "keywords": [
    "component-library",
    "tailwind-vue",
    "vue-components",
    "ui-components",
    "tailwind",
    "tailwindcss",
    "tailwind-css",
    "vuejs",
    "tailwindui",
    "tailwind-ui",
    "tailwind-blocks",
    "css",
    "vue",
    "vue-ui",
    "components",
    "vue-ui",
    "vue-ui-components",
    "blocks",
    "vue-blocks",
    "vue-sections",
    "tailgrids",
    "tailwind-ui",
    "tailwind-sections",
    "tailgrids-vue",
    "design-system"
  ],
  "dependencies": {
    "pinia": "^2.1.7",
    "swiper": "^11.0.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailgrids": "^2.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
